{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "heads": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::head.head",
      "mappedBy": "articles"
    },
    "rank": {
      "type": "integer",
      "default": 1
    },
    "author": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::author.author",
      "inversedBy": "articles"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "content": {
      "type": "richtext",
      "required": true
    },
    "comment_count": {
      "type": "string",
      "unique": true
    },
    "likeCountField": {
      "type": "string",
      "unique": true
    }
  }
}
